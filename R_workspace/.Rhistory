aflow <- read.delim("Aflow_metals_ibz.csv",header=F,sep="\t")
froy_v <- read.delim("Froyen_VASP_metals_ibz.csv",header=F,sep="\t")
froy <- read.delim("Froyen_metals_ibz.csv",header=F,sep="\t")
muel <- read.delim("Mueller_metals_ibz.csv",header=F,sep="\t")
library("scales")
mycol=c("green","red","blue")
opa.a <- 0.2
span.sm <- 0.5
pdf("ErrorComparison.pdf",height=5,width=7)
x<-aflow
x<-log10(x)
plot(x[,1],x[,2],col=alpha(mycol[1],opa.a),xlab="Number of Irreducible K Points",ylab="Error",axes=F)
ticks <- c(-.5,0,1,2,3,4)
labs <- sapply(ticks, function(i) as.expression(bquote(10^ .(i))))
axis(1,at=ticks,labels=labs)
ticks <- seq(0,-6, by=-1)
labs <- sapply(ticks, function(i) as.expression(bquote(10^ .(i))))
axis(2,at=ticks,labels=labs)
ff1<-loess.smooth(x[,1],x[,2],span=span.sm,evaluation=200)
lines(ff1$x,ff1$y,col=mycol[1],lwd=4)
x<-froy
x<-log10(x)
points(x[,1],x[,2],col=alpha(mycol[2],opa.a))
ff2<-loess.smooth(x[,1],x[,2],span=span.sm,evaluation=200)
lines(ff2$x,ff2$y,col=mycol[2],lwd=4)
x<-muel
x<-log10(x)
points(x[,1],x[,2],col=alpha(mycol[3],opa.a))
ff3<-loess.smooth(x[,1],x[,2],span=span.sm,evaluation=200)
x<-froy_v
x<-log10(x)
points(x[,1],x[,2],col=alpha(mycol[3],opa.a))
ff4<-loess.smooth(x[,1],x[,2],span=span.sm,evaluation=200)
write.csv(ff1,file="Aflow_ibz_loess.csv",row.names=FALSE)
write.csv(ff2,file="Froyen_ibz_loess.csv",row.names=FALSE)
write.csv(ff3,file="Muel_ibz_loess.csv",row.names=FALSE)
write.csv(ff4,file="Froyen_VASP_ibz_loess.csv",row.names=FALSE)
dev.off()
exit
exit()
quit
quit(
